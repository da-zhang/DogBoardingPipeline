<pipeline>
  <step name="FindAllStateLinks">
    <id>1</id>
    <inputTextFile>FindAllStateLinksInput</inputTextFile>
    <outputTextFile>AllStateLinks</outputTextFile>
    <elements>
      <element>
        <columnName>StateLink</columnName>
        <locator>//a[@class='text-muted']</locator>
        <locatorType>xpath</locatorType>
        <valueIndicator>Attribute:href</valueIndicator>
      </element>
    </elements>
    <manyPage>false</manyPage>
    <ignore>true</ignore>
    <pageUrl></pageUrl>
  </step>
  <step name="FindAllCitiesLinks">
    <id>2</id>
    <inputTextFile>AllStateLinks</inputTextFile>
    <outputTextFile>AllCitiesLinks</outputTextFile>
    <elements>
      <element>
        <columnName>CityLink</columnName>
        <locator>//ul[@class='list-unstyled']/li/a</locator>
        <locatorType>xpath</locatorType>
        <valueIndicator>Attribute:href</valueIndicator>
      </element>
    </elements>
    <manyPage>false</manyPage>
    <ignore>true</ignore>
    <pageUrl></pageUrl>
  </step>
  <step name="FindAllProfiles">
    <id>3</id>
    <inputTextFile>AllCitiesLinks</inputTextFile>
    <outputTextFile>AllProfiles</outputTextFile>
    <elements>
      <element>
        <columnName>ProfileLink</columnName>
        <locator>//a[@class='sitter-link js-profile-link']</locator>
        <locatorType>xpath</locatorType>
        <valueIndicator>Attribute:href</valueIndicator>
      </element>
    </elements>
    <manyPage>true</manyPage>
    <pageUrl>ProfileListPageUrlTemplate</pageUrl>
    <ignore>false</ignore>
  </step>
  <step name="CollectData">
    <id>4</id>
    <inputTextFile>AllProfiles</inputTextFile>
    <outputTextFile>AllData</outputTextFile>
    <elements>
      <element>
        <columnName>Name</columnName>
        <locator>//span[@itemprop='name']</locator>
        <locatorType>xpath</locatorType>
        <valueIndicator>InnerText</valueIndicator>
      </element>
      <element>
        <columnName>Address</columnName>
        <locator>//div[@class='provider-neighborhood']</locator>
        <locator>xpath</locator>
        <valueIndicator>InnerText</valueIndicator>
      </element>
      <element>
        <columnName>Reviews</columnName>
        <locator>//a[@class='#reviews']</locator>
        <locatorType>xpath</locatorType>
        <valueIndicator>InnerText</valueIndicator>
      </element>
    </elements>
    <manyPage>false</manyPage>
    <pageUrl></pageUrl>
    <ignore>true</ignore>
  </step>
</pipeline>